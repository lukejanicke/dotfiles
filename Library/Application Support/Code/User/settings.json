{
  "[html][css][javascript][json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeLensFontFamily": "\"Iosevka Custom\"",
  "editor.codeLensFontSize": 13,
  "editor.fontFamily": "\"Iosevka Custom\", \"SymbolsCustom Nerd Font\"",
  "editor.fontLigatures": "'dlig', 'calt'",
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.inlayHints.fontSize": 12,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "never",
  "latex-workshop.latex.recipe.default": "LuaLaTeX",
  "latex-workshop.latex.recipes": [
    {
      "name": "LuaLaTeX",
      "tools": ["lualatex"]
    }
  ],
  "latex-workshop.latex.tools": [
    {
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ],
      "command": "lualatex",
      "name": "lualatex"
    }
  ],
  "markdown.preview.fontFamily": "Inter",
  "notebook.markup.fontSize": 13,
  "roo-cline.allowedCommands": [
    "npm test",
    "npm install",
    "tsc",
    "git log",
    "git diff",
    "git show"
  ],
  "terminal.integrated.fontFamily": "\"Iosevka Custom Fixed\", \"SymbolsCustom Nerd Font Mono\"",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": 400,
  "terminal.integrated.fontWeightBold": 700,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.minimumContrastRatio": 1,
  "window.autoDetectColorScheme": true,
  "window.confirmSaveUntitledWorkspace": false,
  "window.systemColorTheme": "auto",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-latte", // Updated automatically with system setting.
  "workbench.preferredDarkColorTheme": "Catppuccin Mocha",
  "workbench.preferredLightColorTheme": "Catppuccin Latte"
}
